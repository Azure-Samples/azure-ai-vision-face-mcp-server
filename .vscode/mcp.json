{
    "servers": {
        "livenessAuthentication": {
            "type": "stdio",
            "command": "node",
            "args": [
                "${workspaceFolder}/liveness-server-typescript/build/stdio.js"
            ],
            "env": {
                "FACEAPI_ENDPOINT": "yourvalue",
                "FACEAPI_KEY": "yourvalue",
                "FACEAPI_WEBSITE": "https://example.azurewebsites.net",
                "SESSION_IMAGE_DIR": "${workspaceFolder}/liveness-server-typescript/build/",
                "VERIFY_IMAGE_FILE_NAME": "${workspaceFolder}/liveness-server-typescript/build/s/sessionImage.jpg",
            }
        }
    }
}